{
  "builds": [
    {
      "src": "migrate.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "templates"
      }
    },
    {
      "src": "core/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "rewrites": [
    {
      "source": "(.*)",
      "destination": "core/wsgi.py"
    }
  ],
  "trailingSlash": true
}